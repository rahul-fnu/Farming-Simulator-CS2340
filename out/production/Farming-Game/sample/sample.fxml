<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #7CFC00;">
         <center>
            <Button mnemonicParsing="false" text="Start Game" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <Label alignment="CENTER" text="Welcome to FarmSim" textAlignment="CENTER" textOverrun="CLIP" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Apple Braille" size="24.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </BorderPane.margin>
            </Label>
         </top>
      </BorderPane>
     



      
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
